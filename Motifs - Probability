def Pr(Motif, Profile):
    # Motif Finding Problem:â€¨ Given a collection of strings, find a set of k - mers, one from each string, that minimizes the score of the
    # resulting
    # Input: A collection of strings Dna and an integer k
    # Output: A collection Motifs of k-mers, one from each string in Dna, minimizing Score(Motifs) amon
    pr = 1
    for i in range(len(Motif)):
        symbol = Motif[i]
        pr *= Profile[symbol][i]
        if pr == 0:
            return 0
    return pr

