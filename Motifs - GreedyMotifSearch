def GreedyMotifSearch(Dna, k, t):
    # Input:  A list of kmers Dna, and integers k and t (where t is the number of kmers in Dna)
    # Output: GreedyMotifSearch(Dna, k, t)
    t = len(Dna)

    BestMotifs = []
    for i in range(0, t):  # loop through col
        BestMotifs.append(Dna[i][0: k])  # setting BestMotifs equal to the first k-mer from each string in Dna.

    for i in range(len(Dna[0]) - k + 1):  # Outer loop,
        Motifs = []
        Motifs.append(Dna[0][i: i + k])  # for each k-mer in first Dna[0] string, ranges over all possible k-mers

        for j in range(1, t):  # Inner loop, iterate through through all strings from second dna[1] string
            P = Profile(Motifs[0: j])  # Create a profile from all motifs in our list to build a profile matrix (in start, we only have 1 Motif)
            Motifs.append(ProfileMostProbablePattern(Dna[j], k, P))  # Profile-most probable motif in dna[i] using the current profile, Add the resulting motif to motif list
            # repeat using the next dna string and the updated motif list

        if Score(Motifs) < Score(
                BestMotifs):  # checks whether Motifs outscores the current best scoring collection of motifs, BestMotifs.
            BestMotifs = Motifs

    return BestMotifs

